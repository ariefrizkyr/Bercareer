<div class="row">
  <div class="col-md-6 col-md-offset-3">

  </div>
</div>
<% @students.each do |student| %>
  <% if student.resume %>
    <%= link_to student do %>
      <%= student.full_name %>
      <% if current_company %>
        <%= link_to 'Send message', new_message_path(to: student.id), class: 'btn btn-default btn-sm' %>
      <% end %><br>
    <% end %>
  <% end %>
<% end %>
